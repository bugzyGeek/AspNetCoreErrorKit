{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ErrorHandlingOptions": {
    "DefaultErrorMessage": "An error occurred.",
    "EnableDetailedErrors": true,
    "LogExceptions": true,
    "UseProblemDetails": true
  }
}
